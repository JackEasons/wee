{
    "extends": "airbnb-base",
	"env": {
		"browser": true
	},
    "plugins": [
        "import"
    ],
	"settings": {
		"import/resolver": {
			"webpack": {
				"config": "./build/webpack.config.js"
			}
		}
	},
	"rules": {
		"comma-dangle": ["error", "never"],
		"consistent-return": 0,
		"eol-last": 0,
		"indent": ["error", "tab"],
		"no-tabs": 0
	}
}