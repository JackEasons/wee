{
  "name": "wee",
  "version": "4.4.2",
  "description": "Blueprint for modern web development",
  "homepage": "https://www.weepower.com",
  "license": "Apache-2.0",
  "moduleRoots": "node_modules/wee-core/scripts",
  "keywords": [
    "css",
    "framework",
    "javascript"
  ],
  "repository": "weepower/wee",
  "bugs": "https://github.com/weepower/wee/issues",
  "browserslist": [
    "last 2 versions",
    "safari >= 10",
    "ie 11"
  ],
  "config": {
    "root": "public",
    "assets": "assets",
    "source": "source",
    "build": "build"
  },
  "scripts": {
    "clean": "rimraf $npm_package_config_root/$npm_package_config_assets",
    "start": "npm run watch",
    "build": "cross-env NODE_ENV=development BASE_PATH=$npm_package_config_source webpack --config $npm_package_config_build/webpack.dev.config.js --progress --hide-modules --display=normal",
    "build:prod": "npm run clean && cross-env NODE_ENV=production BASE_PATH=$npm_package_config_source webpack -p --config $npm_package_config_build/webpack.prod.config.js --progress --hide-modules",
    "watch": "npm run clean && npm run build -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "extract-css-chunks-webpack-plugin": "^3.1.1",
    "imagemin-webpack-plugin": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "postcss-variable-media": "^1.0.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.3",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "suppress-chunks-webpack-plugin": "0.0.4",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2",
    "webpack-chain": "^4.10.0",
    "webpack-clean-obsolete-chunks": "^0.4.0",
    "webpack-cli": "^3.1.0",
    "webpack-manifest-plugin": "^2.0.3",
    "wee-cli": "next"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "es6-promise": "^4.2.4",
    "path-to-regexp": "1.7.0",
    "wee-core": "^2.2.0"
  }
}
